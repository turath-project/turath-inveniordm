{
  "name": "My Dev Container",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "dockerComposeFile": "../docker-compose.full.yml",
  "service": "web-ui",
  "workspaceFolder": "/workspace",
  "extensions": [
    "ms-python.python",
    "ms-azuretools.vscode-docker"
  ],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "/bin/bash"
  },
  "postCreateCommand": "invenio-cli containers start --lock --build --setup -f ../docker-compose.full.yml",
  "shutdownAction": "stopCompose"
}
